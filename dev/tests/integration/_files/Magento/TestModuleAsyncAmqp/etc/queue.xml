    <topic name="multi.topic.queue.topic.c" schema="Magento\TestModuleAsyncAmqp\Model\AsyncTestDataInterface" publisher="test-queue" />
    <consumer name="queueForMultipleTopicsTestC" connection="amqp" queue="queue.for.multiple.topics.test.c" class="Magento\TestModuleAsyncAmqp\Model\RequestHandler" method="process" />
